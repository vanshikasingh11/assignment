function add(...args) {
  let sum = 0;
  for (const arg of args) {
    if (!isNaN(arg) && typeof arg === "number") {
      sum += Number(arg);
    } else if (Array.isArray(arg)) {
      sum += add(...arg); // Recursively call add for array elements
    }
  }
  return sum;
}

