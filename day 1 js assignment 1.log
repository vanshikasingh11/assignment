QUES 1)  Prime Number Compute Function



function isPrime(number) {
  
  if (number <= 1) {
    return false;
  }


  if (number <= 3) {
    return true;
  }

  
  const sqrtNumber = Math.floor(Math.sqrt(number));
  for (let i = 2; i <= sqrtNumber; i++) {
    if (number % i === 0) {
      return false;
    }
  }

  return true;
}

